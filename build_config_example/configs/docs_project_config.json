{
    "localReposDir"      : "build/repos",
    "guideSourceDir"     : "{localReposDir}/{repo}",
    "apiSourceDir"       : "{localReposDir}/{repo}",
    "apiInputDir"        : "build/input",
    "outputDir"          : "build/output",
    "outputProductDir"   : "{outputDir}/{product}",
    "diffOutputDir"      : "{outputDir}/assets/diff/{product}",
    "offlineDocsDir"     : "{outputDir}/downloads",
    "assetsDir"          : "{outputDir}/assets",
    "cssDir"             : "{assetsDir}/css",
    "imagesDir"          : "{assetsDir}/images",
    "jsDir"              : "{assetsDir}/js",
    "resourcesDir"       : "resources",
    "parserConfigPath"   : "configs/doxi/{product}",
    "doxiProcessLinks"   : true,
    "doxiQuiet"          : true,
    "htmlSourceTpl"      : "templates/html-src.hbs",
    "typeCategories"     : [ "class", "configs", "properties", "static-properties",
                           "methods", "static-methods", "events", "vars",
                           "sass-mixins" ],
    "classProps"         : [ "alias", "alternateClassNames", "extends", "mixins", "uses",
                           "singleton", "access", "requires" ],
    "skipClassProps"     : [ "requires", "uses"],
    "memberProps"        : [ "access", "optional", "value", "accessor", "inheritdoc",
                           "deprecatedMessage", "removedMessage", "hide", "localdoc",
                           "preventable", "readonly", "type", "required" ],
    "skipMemberProps"    : ["inheritdoc"],
    "productMenu"        : [
        "myproduct"
    ],
    "buildExceptions" : {
    },
    "products"         : {
        "guides" : {
            "repo"      : "docs-example-guides",
            "remoteUrl" : "git@github.com:sencha/{repo}.git"
        },
        "myproduct" : {
                "repo"           : "docs-example-project",
                "remoteUrl"      : "git@github.com:sencha/{repo}.git",
                "title"          : "MyProduct",
                "currentVersion" : "1.5",
                "hasVersions"    : true,
                "hasApi"         : true,
                "sourceProduct"  : "myproduct",
                "sourceVer"      : {
                    "1.0" : "1.0",
                    "1.5" : "1.5"
            },
            "productMenu" : [
                "1.0", "1.5"
            ]
        }
    },

    "productIndex" : {
        "main" : {
            "hasVersions" : false
        },
        "myproduct" : {
            "hasVersions" : true
        }
    },
    "normalizedProductList" : {
        "main"      : "main",
        "myproduct" : "myproduct"
    }
}
