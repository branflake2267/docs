{
  "title": "Sencha Architect Docs",

  "footer": "Sencha Architect Docs | <a href='https://www.sencha.com/legal/terms-of-use/' target='_blank'>Terms of Use</a>",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/architect/3' /><meta name='description' content='Sencha Architect Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/architect/3/",

  "compress": false,

  "destination" : {
    "type"  : "path",
    "value" : "./output/architect-output/"
  },

  "basePaths" : "./configs/basePaths.json",

  "stylesheet"  : "./modules/base/css/styles.css",

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.0/6.0.1-classic/#!/api/",
    "touch" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/",
    "modern" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/"
  },

  "hometemplate": {
    "path": "./product-templates/architect/",
    "name": "architect-template.hbs"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/architect-json/"
  },

  "guides" : {
    "version": "3.0",
    "path"   : "../../sencha-documentation/markdown/src/main/markdown/architect",
    "output" : "guides",
    "template" : "./product-templates/architect/guide/template.hbs",
    "homeTemplate": "./product-templates/architect/guide/home-template.hbs"
  }
}

