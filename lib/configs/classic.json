{
    "title": "Ext JS",

    "meta" : "Classic Toolkit",

    "otherToolkit" : "modern",
    "otherToolkitConfig" : "modern",

    "headhtml": [
        "<link rel='canonical' href='http://docs.sencha.com/extjs/6.0/' /><meta name='description' content='Ext JS Classic API Documentation from Sencha.' />"
    ],

    "docroot": "http://docs.sencha.com/extjs/6.0.2/",

    "destination": {
        "type": "path",
        "value": "./output/extjs/"
    },

    "input": {
        "type": "path",
        "value": "./input/classic-json/"
    },

    "frameworks" : {
        "ext" : "http://docs.sencha.com/extjs/6.0.2/classic/",
        "touch" : "http://docs.sencha.com/extjs/6.0.2/modern/",
        "modern" : "http://docs.sencha.com/extjs/6.0.2/modern/"
    },

    "guides": {
        "version": "6.0",
        "path": "../modules/guides/extjs",
        "config": "config60.json",
        "output": "guides",
        "template": "./modules/base/tpls/main.hbs",
        "homeTemplate": "./modules/base/tpls/main.hbs"
    },

    "hasApi" : true,

    "search": {
        "config": "cmd",
        "pversion": "6.x"
    }
}
