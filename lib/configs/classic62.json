{
    "title": "Ext JS",

    "meta" : "Classic Toolkit",

    "otherToolkit" : "modern",

    "headhtml": [
        "<link rel='canonical' href='http://docs.sencha.com/extjs/6.2.0-classic/' /><meta name='description' content='Ext JS Classic API Documentation from Sencha.' />"
    ],

    "docroot": "http://docs.sencha.com/extjs/6.2.0-classic/",

    "compress": true,

    "destination": {
        "type": "path",
        "value": "./output/extjs/"
    },

    "input": {
        "type": "path",
        "value": "./input/classic-json/"
    },

    "stylesheet": "./modules/base/css/styles.css",

    "template": "./modules/base/tpls/main.hbs",

    "imagePath": "../modules/SDK/docs/resources",

    "hometemplate": "./modules/base/tpls/main.hbs",

    "frameworks" : {
        "ext" : "http://docs.sencha.com/extjs/6.2.0-classic/",
        "touch" : "http://docs.sencha.com/extjs/6.2.0-modern/",
        "modern" : "http://docs.sencha.com/extjs/6.2.0-modern/"
    },

    "guides": {
        "version": "6.2",
        "path": "../modules/sencha-documentation/markdown/src/main/markdown/extjs",
        "config": "config62.json",
        "output": "guides",
        "template": "./modules/base/tpls/main.hbs",
        "homeTemplate": "./modules/base/tpls/main.hbs"
    },

    "allClasses": "classic-all-classes.json",

    "hasGuides" : true,
    "hasApi" : true
}
