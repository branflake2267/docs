{
  "title": "Ext JS",

  "meta" : "Classic Toolkit",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/extjs/5.1/' /><meta name='description' content='Ext JS Classic API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/extjs/5.1/5.1.2-extjs/",

  "compress": true,

  "destination": {
    "type": "path",
    "value": "./output/extjs/"
  },

  "input": {
    "type": "path",
    "value": "./input/extjs-json/"
  },

  "stylesheet": "./modules/base/css/styles.css",

  "template": "./modules/base/tpls/main.hbs",

  "imagePath": "../modules/SDK/ext/docs/resources",

  "hometemplate": "./modules/base/tpls/main.hbs",

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.0/6.0.1-classic/#!/api/",
    "touch" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/",
    "modern" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/"
  },

  "guides": {
    "version": "5.1",
    "path": "../modules/sencha-documentation/markdown/src/main/markdown/extjs",
    "config": "config51.json",
    "output": "guides",
    "template": "./modules/base/tpls/main.hbs",
    "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "allClasses": "all-classes.json",

  "hasGuides" : true,
  "hasApi" : true
}
