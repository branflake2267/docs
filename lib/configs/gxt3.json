  "hometemplate": "./modules/base/tpls/main.hbs",

  "guides" : {
    "version": "3.x",
    "path"   : "../modules/sencha-documentation/markdown/src/main/markdown/gxt",
    "output" : "guides",
    "config": "config3.json",
    "template" : "./modules/base/tpls/main.hbs",
    "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "hasGuides" : true
}

