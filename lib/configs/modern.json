{
  "title": "Ext JS 6.0.1 - Modern Toolkit",

  "footer": "Ext JS 6.0.1 API Docs - Modern Toolkit | <a href='https://www.sencha.com/legal/terms-of-use/' target='_blank'>Terms of Use</a>",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/extjs/6.0/' /><meta name='description' content='Ext JS 6.0.1 Classic API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/extjs/6.0/6.0.1-modern/",

  "compress": false,

  "destination" : {
    "type"  : "path",
    "value" : "./output/modern-toolkit-output/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/modern-json/"
  },

  "basePaths" : "./configs/basePaths.json",

  "stylesheet"  : "./modules/base/css/styles.css",

  "template"    : "./modules/json-parser/template.hbs",

  "imagePath": "../docs/resources",

  "hometemplate": {
    "path": "./product-templates/",
    "name": "ext/classic/extjs-template.hbs"
  },

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.0/6.0.1-classic/#!/api/",
    "touch" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/",
    "modern" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/"
  },

  "guides" : {
    "version": "6.0",
    "path"   : "../../sencha-documentation/markdown/src/main/markdown/extjs",
    "output" : "guides",
    "template": "./product-templates/ext/classic/guide/template.hbs",
    "homeTemplate": "./product-templates/ext/classic/guide-home-template.hbs"
  },

  "allClasses": "modern-all-classes.json"
}
