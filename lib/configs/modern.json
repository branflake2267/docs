{
  "title": "Ext JS",

  "meta" : "Modern Toolkit",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/extjs/6.0/' /><meta name='description' content='Ext JS Classic API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/extjs/6.0/6.0.1-modern/",

  "compress": false,

  "destination" : {
    "type"  : "path",
    "value" : "./output/modern-toolkit-output/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/modern-json/"
  },

  "stylesheet"  : "./modules/base/css/styles.css",

  "template"    : "./modules/base/tpls/main.hbs",

  "imagePath": "../modules/SDK/docs/resources",

  "hometemplate": "./modules/base/tpls/main.hbs",

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.0/6.0.1-classic/#!/api/",
    "touch" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/",
    "modern" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/"
  },

  "guides" : {
    "version": "6.0",
    "path"   : "../modules/sencha-documentation/markdown/src/main/markdown/extjs",
    "output" : "guides",
    "template": "./modules/base/tpls/main.hbs",
    "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "allClasses": "modern-all-classes.json",

  "hasGuides" : true,
  "hasApi" : true
}
