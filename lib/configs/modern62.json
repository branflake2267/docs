{
  "title": "Ext JS",

  "meta" : "Modern Toolkit",

  "otherToolkit" : "classic",
    "otherToolkitConfig" : "classic62",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/extjs/6.2.0' /><meta name='description' content='Ext JS Classic API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/extjs/6.2.0",

  "destination" : {
    "type"  : "path",
    "value" : "./output/extjs/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/modern-json/"
  },

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.2.0/classic/",
    "touch" : "http://docs.sencha.com/extjs/6.2.0/modern/",
    "modern" : "http://docs.sencha.com/extjs/6.2.0/modern/"
  },

  "guides" : {
    "version": "6.2",
    "path"   : "../modules/guides/extjs",
    "config": "config62.json",
    "output" : "guides",
    "template": "./modules/base/tpls/main.hbs",
    "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "hasApi" : true,

    "search": {
        "config": "cmd",
        "pversion": "6.x"
    }
}
