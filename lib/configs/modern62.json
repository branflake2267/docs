{
  "title": "Ext JS",

  "meta" : "Modern Toolkit",

  "otherToolkit" : "classic",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/extjs/6.2.0-modern' /><meta name='description' content='Ext JS Classic API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/extjs/6.2.0-modern/",

  "compress": true,

  "destination" : {
    "type"  : "path",
    "value" : "./output/extjs/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/modern-json/"
  },

  "stylesheet"  : "./modules/base/css/styles.css",
  "treestyle" : "./modules/base/css/treeview.css",

  "template"    : "./modules/base/tpls/main.hbs",

  "imagePath": "../modules/SDK/docs/resources",

  "hometemplate": "./modules/base/tpls/main.hbs",

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.2.0-classic/",
    "touch" : "http://docs.sencha.com/extjs/6.2.0-modern/",
    "modern" : "http://docs.sencha.com/extjs/6.2.0-modern/"
  },

  "guides" : {
    "version": "6.2",
    "path"   : "../modules/sencha-documentation/markdown/src/main/markdown/extjs",
    "config": "config62.json",
    "output" : "guides",
    "template": "./modules/base/tpls/main.hbs",
    "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "allClasses": "modern-all-classes.json",

  "hasGuides" : true,
  "hasApi" : true
}
