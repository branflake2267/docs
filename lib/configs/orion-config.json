{
  "title": "Sencha Test",

  "footer": "Sencha Test Docs | <a href='https://www.sencha.com/legal/terms-of-use/' target='_blank'>Terms of Use</a>",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/sencha_test/' /><meta name='description' content='Sencha Test API Documentation from Sencha.' />"
  ],

  "compress": false,

  "destination" : {
    "type"  : "path",
    "value" : "./output/orion-output/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/orion-json/"
  },

  "basePaths" : "./configs/basePaths.json",

  "stylesheet"  : "./modules/base/css/styles.css",

  "template"    : "./modules/json-parser/template.hbs",

  "hometemplate": {
    "path": "./product-templates/",
    "name": "test/orion-template.hbs"
  },

  "guides" : {
      "version": "1.0",
      "path"   : "../../sencha-documentation/markdown/src/main/markdown/sencha_test",
      "output" : "guides",
      "template" : "./product-templates/test/guide/template.hbs",
      "homeTemplate": "./product-templates/test/guide/home-template.hbs"

  },

  "allClasses": "orion-all-classes.json"
}

