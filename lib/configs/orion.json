{
  "title": "Sencha Test",

  "description": "Sencha Test is a new addition to the Sencha platform that will help you create unit and end-to-end functional tests, and execute them on multiple browsers.",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/sencha_test/' /><meta name='description' content='Sencha Test API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/sencha_test/",

  "compress": true,

  "destination" : {
    "type"  : "path",
    "value" : "./output/sencha_test/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/orion-json/"
  },

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.0/6.0.1-classic/#!/api/",
    "touch" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/",
    "modern" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/"
  },

  "stylesheet"  : "./modules/base/css/styles.css",

  "template"    : "./modules/base/tpls/main.hbs",

  "hometemplate": "./modules/base/tpls/main.hbs",

  "guides" : {
      "version": "1.0",
      "path"   : "../modules/sencha-documentation/markdown/src/main/markdown/sencha_test",
      "output" : "guides",
      "template" : "./modules/base/tpls/main.hbs",
      "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "allClasses": "orion-all-classes.json",

  "hasGuides" : true,
  "hasApi" : true
}

