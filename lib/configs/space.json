{
  "title": "Sencha Web Application Manager",

  "footer": "Sencha Web Application Manager API Docs | <a href='https://www.sencha.com/legal/terms-of-use/' target='_blank'>Terms of Use</a>",

  "headhtml": [
    "<link rel='canonical' href='http://docs.sencha.com/webappmgr/' /><meta name='description' content='Sencha Web Application Manager API Documentation from Sencha.' />"
  ],

  "docroot": "http://docs.sencha.com/webappmgr/",

  "compress": true,

  "destination" : {
    "type"  : "path",
    "value" : "./output/space-output/"
  },

  "input" : {
    "type"  : "path",
    "value" : "./input/space-json/"
  },

  "stylesheet"  : "./modules/base/css/styles.css",

  "template"    : "./modules/base/tpls/main.hbs",

  "hometemplate": "./modules/base/tpls/main.hbs",

  "frameworks" : {
    "ext" : "http://docs.sencha.com/extjs/6.0/6.0.1-classic/#!/api/",
    "touch" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/",
    "modern" : "http://docs.sencha.com/extjs/6.0/6.0.1-modern/#!/api/"
  },

  "guides" : {
    "version": "6.0",
    "path"   : "../modules/sencha-documentation/markdown/src/main/markdown/webappmgr",
    "output" : "guides",
    "template": "./modules/base/tpls/main.hbs",
    "homeTemplate": "./modules/base/tpls/main.hbs"
  },

  "allClasses": "space-all-classes.json",

  "hasGuides" : true,
  "hasApi" : true
}