<!DOCTYPE html>
<html>
<head>
    <title>{{#if name}}{{{name}}}{{else}}Docs{{/if}} | {{{title}}} {{{numVer}}} {{#if meta}}- {{{meta}}} {{/if}}</title>
    <!--  Generated on {{{date}}} -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta property="og:image" content="{{imagePath}}sencha-200.png" />
    <meta property="og:title" content="{{#if name}}{{{name}}}{{else}}Docs{{/if}} | {{{title}}} {{{numVer}}} {{#if meta}}- {{{meta}}} {{/if}}" />
    <meta property="og:description" content="{{description}}" />
    <meta property="og:site_name" content="Sencha Documentation" />
    <meta property="og:url" content="{{canonical}}" />
    <meta name="description" content="{{{description}}}">
    <style>
        /*!
         *  Font Awesome 4.5.0 by @davegandy - http://fontawesome.io - @fontawesome
         *  License - http://fontawesome.io/license (Font: SIL OFL 1.1, CSS: MIT License)
         */
        /* FONT PATH
         * -------------------------- */
        @font-face {
            font-family: 'FontAwesome';
            src: url('{{cssPath}}fonts/fontawesome-webfont.eot?v=4.5.0');
            src: url('{{cssPath}}fonts/fontawesome-webfont.eot?#iefix&v=4.5.0') format('embedded-opentype'),
            url('{{cssPath}}fonts/fontawesome-webfont.woff2?v=4.5.0') format('woff2'),
            url('{{cssPath}}fonts/fontawesome-webfont.woff?v=4.5.0') format('woff'),
            url('{{cssPath}}fonts/fontawesome-webfont.ttf?v=4.5.0') format('truetype'),
            url('{{cssPath}}fonts/fontawesome-webfont.svg?v=4.5.0#fontawesomeregular') format('svg');
            font-weight: normal;
            font-style: normal;
        }

    </style>
    <link rel="stylesheet" type="text/css" href="{{cssPath}}prism.css">
    {{#if stylesheet}}
        <link rel="stylesheet" type="text/css" href="{{cssPath}}{{stylesheet}}">
    {{/if}}
    {{#headhtml}}
        {{{.}}}
    {{/headhtml}}
</head>
<body class="{{#isHome}}is-home{{/isHome}}{{#isGuide}} is-guide{{/isGuide}}{{#isApi}} is-api{{/isApi}}{{#isLanding}} is-landing{{/isLanding}} {{#if hasGuides}} has-guides{{/if}} {{#if hasApi}} has-api{{/if}}">
    <script type="text/javascript">
        function getOS() {
            var userAgent = navigator.userAgent || navigator.vendor || window.opera;

            if(/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                return 'ios';
            } else if(userAgent.match(/Android/i)) {
                return 'android';
            }
        }

        var isIE8 = false;

        document.body.className += ' ' + getOS();
    </script>
    <!--[if IE 8]>
    <script type='text/javascript'>
        isIE8 = true;
    </script>
    <![endif]-->
    <script type="text/javascript">
        var ExtL = ExtL || {},
            specificId = '{{specificId}}',
            {{#if fiddleId}}fiddleId = {{fiddleId}},{{/if}}
            isClassTemplate = {{#if isApi}}true{{else}}false{{/if}},
            isHome = {{#if isHome}}true{{else}}false{{/if}},
            isLanding = {{#if isLanding}}true{{else}}false{{/if}},
            isApi = {{#if isApi}}true{{else}}false{{/if}},
            isGuide = {{#if isGuide}}true{{else}}false{{/if}},
            hasApi = {{#if hasApi}}true{{else}}false{{/if}},
            hasGuides = {{#if hasGuides}}true{{else}}false{{/if}},
            singleton = {{#if singleton}}true{{else}}false{{/if}},
            pageName = '{{name}}',
            homePath = '{{homePath}}',
            product = '{{product}}',
            pversion = '{{pversion}}',
            hasVersions = {{hasVersions}};

            if (!isIE8) {
                document.write('<script type=\"text/javascript\" src=\"{{jsPath}}prism.js\"><\/script>');
            }
    </script>
    <script type="text/javascript" src="{{jsPath}}ace.js"></script>
    <script type="text/javascript" src="{{jsPath}}beautify.js"></script>
    <script type="text/javascript" src="{{jsPath}}app.js"></script>
    {{> helpPartial}}
    <div id="header">
        <div class="logo">
            <a href="http://docs.sencha.com"><img height="32" src="{{imagePath}}sencha_logo_thumb.png"/></a>
        </div>
        <span id="mobile-main-nav-menu-btn" class="fa fa-bars"></span>
        <span data-tip="Product / Version Menu" class="fa fa-chevron-circle-down product-menu-btn tooltip tooltip-tl-bl landing-product-menu-btn"><div class="callout callout-b"></div></span>
        {{#isApi}}
            <a class="page-title {{classType}}" href="{{{fileName}}}" target="_blank">{{{name}}}</a>
        {{/isApi}}
        {{#isHome}}
            <div class="page-title">{{{title}}} {{{numVer}}} {{#if meta}}- {{{meta}}}{{/if}}</div>
        {{/isHome}}
        {{#isGuide}}
            <div class="page-title">{{{name}}}</div>
        {{/isGuide}}
        {{#if toolkitLink}}
            <a class="toolkit-switch" href="{{toolkitLink}}">{{toolkit}} Toolkit</a>
        {{/if}}
        <span id="mobile-context-menu-btn" class="fa fa-cog"></span>
    </div>
    <div id="centerContent">
        <div id="class-tree-ct" class="class-tree">
            <h2 class="product-header">
                <div class="logo">
                    <a href="http://docs.sencha.com"><img src="{{imagePath}}sencha_logo1x.png"/></a>
                </div>
                <a href="{{homePath}}index.html">{{{title}}} {{{numVer}}} {{#if meta}}- {{{meta}}}{{/if}}</a>
                <span data-tip="Product / Version Menu" class="fa fa-chevron-circle-down product-menu-btn  tooltip tooltip-tl-bl"><div class="callout callout-b"></div></span>
            </h2>
            <div id="tree-header" class="pre-load">
                <div id="guideTab" class="toolbarHeaderButton{{#isGuide}} active-tab{{/isGuide}}">
                    Guides
                </div>
                <div id="apiTab" class="toolbarHeaderButton{{#isApi}} active-tab{{/isApi}}">
                    API
                </div>
                <div id="hide-class-tree"></div>
            </div>
            <div id="tree-scroller">
                <div id="tree" class="show-private show-protected is-tree">
                    {{#if apiJson}}{{#each apiJson}}{{> treeNodePartial}}{{/each}}{{/if}}
                    <div class="fa fa-plus toggle-tree hide tooltip tooltip-tr-br" data-expand="Expand All Classes" data-collapse="Collapse All Classes"><div class="callout callout-b"></div></div>
                </div>
                <div id="guide-tree" class="hide is-tree">
                    {{#if guideJson}}{{#each guideJson}}{{> treeNodePartial}}{{/each}}{{/if}}
                    <div class="fa fa-plus toggle-tree hide tooltip tooltip-tr-br" data-expand="Expand All Guides" data-collapse="Collapse All Guides"><div class="callout callout-b"></div></div>
                </div>
            </div>
        </div>
        <a href="#" id="back-to-top">top </a>
        <div id="rightMembers" class="">
            {{#isHome}}
                <div class="context-menu-ct">
                    <div class="context-header">
                        <div id="hide-context-menu" class="fa fa-caret-right"></div>
                    </div>
                    <div class="search">
                        <form autocomplete="off">
                            <input type="text" type="search" placeholder="search" id="searchtext">
                            <i id="filtersubmit" class="fa fa-search"></i>
                            <span id="help-btn" data-tip="Docs Help" class="tooltip tooltip-tr-br"><div class="callout callout-b"></div></span>
                        </form>
                    </div>
                    <!-- context content added at runtime -->
                </div>
            {{/isHome}}
            {{#if isHome}}{{> homePartial}}{{/if}}
            {{#if isGuide}}{{> guidePartial}}{{/if}}
            {{#if isApi}}{{> apiPartial}}{{/if}}
        </div>
    </div>
    <h2 class="product-header"><a href="{{homePath}}index.html">{{{title}}} {{{numVer}}} {{#if meta}}- {{{meta}}}{{/if}}</a>
        <span data-tip="Product / Version Menu" class="fa fa-chevron-circle-down product-menu-btn tooltip tooltip-tl-bl"><div class="callout callout-b"></div></span>
        <div id="peekaboo-input" class="peekaboo-input">
            <input type="text" id="mobile-input" />
        </div>
    </h2>

    <div id="product-tree-ct" class="hide">
        <div id="product-tree">
            <div class="product-name-ct">
                {{#each productMapJson}}
                    <div id="product-menu-{{id}}" class="product-name-item">{{name}}</div>
                {{/each}}
            </div>
            {{#each productMapJson}}
                <div class="product-version-ct hide {{id}}">
                    {{#if children}}
                        <h2>{{name}}</h2>
                        {{#each children}}
                            {{#if children}}
                            <h3>{{name}}</h3>
                                {{#each children}}
                                    {{#if compound}}
                                        <div class="compound-product-item">{{{name}}}
                                            {{#toolkits}}
                                                <a {{#if preparedHref}} href="{{preparedHref}}"{{/if}}>{{{name}}}</a>
                                            {{/toolkits}}
                                        </div>
                                    {{else}}
                                        <a class="product-item"{{#if preparedHref}} href="{{preparedHref}}"{{/if}}>{{{name}}}</a>
                                    {{/if}}
                                {{/each}}
                            {{else}}
                                <a class="product-item"{{#if preparedHref}} href="{{preparedHref}}"{{/if}}>{{{name}}}</a>
                            {{/if}}
                        {{/each}}
                    {{else}}
                        <h2>{{name}}</h2>
                        <a {{#if preparedHref}} href="{{preparedHref}}"{{/if}}>{{{name}}}</a>
                    {{/if}}
                </div>
            {{/each}}
        </div>
    </div>

</body>
{{#if hasApi}}
    <script type="text/javascript" src="{{jsPath}}searchIndex.js"></script>
{{/if}}
{{#if hasGuides}}
    <script type="text/javascript" src="{{jsPath}}guideSearch.js"></script>
{{/if}}
<!--[if lt IE 9 ]>
<script type="text/javascript" src="{{jsPath}}media-query-poly.js"></script>
<style>
    span.da-inline-fiddle-nav-fiddle.x-fa.fa-play-circle {
        display: none;
    }
</style>
<![endif]-->
