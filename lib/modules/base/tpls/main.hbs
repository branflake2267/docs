<!DOCTYPE html>
<html>
<head>
    <title>{{{title}}}</title>
    <!--  Generated on {{{date}}} -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta property="og:image" content="{{imagePath}}sencha-200.png" />
    <meta name="description" content="{{{description}}}">
    {{#if stylesheet}}
        <link rel="stylesheet" type="text/css" href="{{cssPath}}{{stylesheet}}">
    {{/if}}
    {{#headhtml}}
        {{{.}}}
    {{/headhtml}}
</head>
<body>
<div id="header">
    <div class="logo">
        <a href="http://docs.sencha.com"><img src="{{imagePath}}sencha_logo1x.png"/></a>
    </div>
    <h2><a href="{{homePath}}index.html">{{{title}}}</a>
        <span id="menu-icon" class="menu-icon"></span>
        <span id="search-icon" class="search-icon"></span>
        <div id="peekaboo-input" class="peekaboo-input">
            <input type="text" id="mobile-input" />
        </div>
    </h2>
    <div class="search">
        <form autocomplete="off"><input type="text" type="search" placeholder="search" id="searchtext"></form>
    </div>
</div>
<div id="centerContent">
    <div id="class-tree-ct">
        <div id="tree-header">
            <div id="apiTab" class="toolbarHeaderButton{{#isApi}} active-tab{{/isApi}}">
                api docs
            </div>
            <div id="guideTab" class="toolbarHeaderButton{{#isGuide}} active-tab{{/isGuide}}">
                guides
            </div>
            <div id="hide-class-tree"></div>
        </div>
        <div id="tree-scroller">
            <div id="tree" class="tree-shown hide"></div>
            <div id="guide-tree" class="tree-shown"></div>
        </div>
    </div>
    <a href="#" id="back-to-top">top </a>
    <div id="rightMembers">
        {{#if isHome}}{{> homePartial}}{{/if}}
        {{#if isGuide}}{{> guidePartial}}{{/if}}
        {{#if isApi}}{{> apiPartial}}{{/if}}
    </div>
</div>
<div id="footer">
    <p>
        <a href="https://www.facebook.com/senchainc/" target="_blank"><img class="social" src="{{imagePath}}fb.png"/></a>
        <a href="https://www.linkedin.com/company/sencha-inc" target="_blank"><img class="social" src="{{imagePath}}in.png"/></a>
        <a href="https://twitter.com/Sencha" target="_blank"><img class="social" src="{{imagePath}}twt.png"/></a>
        {{{footer}}} | <a href='{{{docroot}}}sitemap.html'>sitemap</a>
    </p>
</div>
</body>
<script type="text/javascript">
    var ExtL   = ExtL || {},
        isClassTemplate = {{#if isApi}}
            true
        {{else}}
            false
        {{/if}},
        isHome = {{#if isHome}}
            true
        {{else}}
            false
        {{/if}},
        isApi = {{#if isApi}}
            true
        {{else}}
            false
        {{/if}},
        isGuide = {{#if isGuide}}
            true
        {{else}}
            false
        {{/if}};
</script>
{{#if hasApi}}
    <script type="text/javascript" src="{{jsPath}}tree.js"></script>
    <script type="text/javascript" src="{{jsPath}}searchIndex.js"></script>
{{/if}}
{{#if hasGuides}}
<script type="text/javascript" src="{{jsPath}}guide-tree.js"></script>
<script type="text/javascript" src="{{jsPath}}guideSearch.js"></script>
{{/if}}
<script type="text/javascript" src="{{jsPath}}ace.js"></script>
<script type="text/javascript" src="{{jsPath}}app.js"></script>
<script>
    document.onreadystatechange = function () {
        if (document.readyState == "interactive") {
            guideTree.expandTreeToClass();
        }
    }

    if (ExtL.treeDataGuide) {
        guideTree = new TreeView(ExtL.treeDataGuide, 'guide-tree', '{{{pathPrefix}}}');
    }
</script>
