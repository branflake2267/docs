<div class="tree-leaf tree-member{{#access}}-{{{.}}}{{/access}}">
    <div class="tree-leaf-content{{#if leaf}} isLeaf{{else}} isNotLeaf{{/if}}{{#if displayNew}} display-new{{/if}} {{#if guideTlkit}}{{guideTlkit}}-decoration{{/if}}" id="{{#if isSingleton}}singleton{{else}}{{#if leaf}}node{{else}}branch{{/if}}{{/if}}-{{elIdSlug}}" isLeaf="{{leaf}}">
        <span class="tree-expando{{#if leaf}} hidden{{/if}}">â–¸</span>
        {{#if isSingleton}}
            <span class="singleton-type"></span>
        {{else}}
            {{#if children}}
                <span class="folder-type"></span>
            {{else}}
                {{#if guideTlkit}}
                    <span class="{{guideTlkit}}-guide guide"></span>
                {{else}}
                    <span class="{{#if type}}{{type}}{{else}}class{{/if}}-type"></span>
                {{/if}}
            {{/if}}
        {{/if}}
        {{#if compound}}
            <span class="tree-leaf-text">{{{name}}}
                {{#toolkits}}
                    <a {{#if preparedHref}} href="{{preparedHref}}"{{/if}}>{{{name}}}</a>
                {{/toolkits}}
            </span>
        {{else}}
            <a class="tree-leaf-text"{{#if preparedHref}} href="{{preparedHref}}"{{/if}}>{{{name}}}</a>
        {{/if}}
    </div>
    {{#if children}}
        <div class="tree-child-leaves hidden">
            {{#each children}}
                {{> treeNodePartial}}
            {{/each}}
        </div>
    {{/if}}
</div>
