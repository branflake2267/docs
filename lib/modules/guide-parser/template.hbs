<!DOCTYPE html>
<html>
<head>
    <title>{{{name}}}</title>
    <!--  Generated on {{{date}}} -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {{#if stylesheet}}<link rel="stylesheet" type="text/css" href="{{pathPrefix}}{{stylesheet}}">{{/if}}
</head>
<body>
<div id="header">
    <div class="logo"><img src="../{{pathPrefix}}home-images/sencha_logo1x.png"/></div>
    <h2>{{{name}}}
        <div class="class" style="display: none">{{nodeId}}</div>
        <span id="menu-icon" class="menu-icon"></span>
        <span id="search-icon" class="search-icon"></span>
        <div id="peekaboo-input" class="peekaboo-input">
            <input type="text" id="mobile-input" />
        </div>
    </h2>
    <div class="search">
        <form autocomplete="off"><input type="text" type="search" placeholder="search" id="searchtext"></form>
    </div>
    <div class="resourceLinks">
        <a href="../{{pathPrefix}}index.html"><div class="toolbarButton">
            api docs
            <div class="nav-btn-mask"></div>
        </div></a>
        <a href="./{{pathPrefix}}index.html"><div class="toolbarButton">
            guides
            <div class="nav-btn-mask"></div>
        </div></a>
    </div>
</div>
<div id="centerContent">
    <div id="class-tree-ct">
        <div id="tree-header">
            <span class="tree-header-text">All Guides</span>
            <div id="hide-class-tree"></div>
        </div>
        <div id="tree" class="tree-shown"></div>
    </div>
    <a href="#" id="back-to-top">top </a>
    <div id="rightMembers">
        <div class="classText">
            {{#if toc}}
                <div class="toc">
                    Contents 
                    {{#each toc}}
                        <a class="toc-page-{{toLowerCase this.tag}}" href="#{{this.id}}">{{this.name}}</a>
                    {{/each}}
                </div>
            {{/if}}
            {{{content}}}
        </div>
    </div>
</div>
<div id="footer">
    <p>
        <a href="https://www.facebook.com/senchainc/" target="_blank"><img class="social" src="../{{pathPrefix}}home-images/fb.png"/></a>
        <a href="https://twitter.com/Sencha" target="_blank"><img class="social" src="../{{pathPrefix}}home-images/in.png"/></a>
        <a href="https://www.linkedin.com/company/sencha-inc" target="_blank"><img class="social" src="../{{pathPrefix}}home-images/twt.png"/></a>
        {{{footer}}}
    </p>
</div>
</body>
<script type="text/javascript">
    var ExtL = ExtL || {};
    isHome = false;
</script>
<script type="text/javascript" src="{{pathPrefix}}js/guide-tree.js"></script>
<script type="text/javascript" src="{{pathPrefix}}{{homejs}}"></script>
<script type="text/javascript" src="../{{pathPrefix}}js/searchIndex.js"></script>
<script>
    document.onreadystatechange = function () {
        if (document.readyState == "interactive") {
            expandTreeToClass();
        }
    }

    if (ExtL.treeData) {
        tree = new TreeView(ExtL.treeData, 'tree', '{{{pathPrefix}}}');
    }

    function expandTreeToClass() {
        var name = document.querySelector('.class').innerHTML;
        tree.expandTo('[data-item*="'+ name + '"][isLeaf="true"]');
    };
</script>
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-1396058-28', 'auto');
    ga('send', 'pageview');

</script>
</html>