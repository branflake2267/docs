<!DOCTYPE html>
<html>
<head>
    <title>{{{name}}}</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {{#if stylesheet}}<link rel="stylesheet" type="text/css" href="{{pathPrefix}}{{stylesheet}}">{{/if}}
</head>
<body class="condensed-header">
<div id="header">
    <h1>{{{name}}}<span id="menu-icon" class="menu-icon"></span></h1>
    <div class="search">
        <form autocomplete="off"><input type="text" type="search" placeholder="search" id="searchtext"></form>
    </div>
    <div class="resourceLinks">
        <a href="../index.html"><div class="toolbarButton">
            api docs
            <div class="nav-btn-mask"></div>
        </div></a>
        <a href="./index.html"><div class="toolbarButton">
            guides
            <div class="nav-btn-mask"></div>
        </div></a>
    </div>
</div>
<div id="centerContent">
    <div id="class-tree-ct">
        <div id="tree-header">
            All Guides
        </div>
        <div id="tree" class="tree-shown"></div>
    </div>
    <a href="#" id="back-to-top">top </a>
    <div id="rightMembers">
        <div class="classText">
            {{#if toc}}
                <div class="toc">
                    {{#each toc}}
                        <div class="toc-item"><a href="#{{this.id}}">{{this.name}}</a></div>
                    {{/each}}
                </div>
            {{/if}}
            {{{content}}}
        </div>
    </div>
</div>
<div id="footer">{{{footer}}} Generated on {{{date}}}</a></div>
</body>
<script type="text/javascript">
    var ExtL = ExtL || {},
        isHome = true;

    ExtL.treeData = {{{tree}}};
</script>
<script type="text/javascript" src="../js/app.js"></script>
<script type="text/javascript" src="{{pathPrefix}}{{homejs}}"></script>
<script>
    if (ExtL.treeData) {
        tree = new TreeView(ExtL.treeData, 'tree', '{{{pathPrefix}}}');
    }
</script>
</html>