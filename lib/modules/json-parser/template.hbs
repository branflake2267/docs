<html>
<head>
    <title>{{{name}}}</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="css/app.css">
</head>
<body>
<h1 class='{{classType}}'>{{{name}}}
    {{#classAlias}}
        <span class='alias'>alias: {{{.}}}</span>
    {{/classAlias}}
</h1>
<div class="classHead">
    <div class="classMetaHeader">
        Related Classes <a class="button" href="javascript:void(0);" onClick="ExtL.onExpandToggleClick(this);">collapse</a>
    </div>
    <div class="classMeta">
        {{#if altNames}}
            <div class="classMetaColumn">
                <h3>Alternate Names</h3>
                <p class="list">{{{altNames}}}</p>
            </div>
        {{/if}}
        {{#if mixins}}
            <div class="classMetaColumn">
                <h3>Inherited Mixins</h3>
                <p class="list">{{{mixins}}}</p>
            </div>
        {{/if}}
        {{#if requires}}
            <div class="classMetaColumn">
                <h3>Requires</h3>
                <p class="list">{{{requires}}}</p>
            </div>
        {{/if}}
    </div>
</div>
<div id="centerContent">
    <div id="class-tree-ct">
        <div id="tree-header">All Classes</div>
        <div id="tree" class="tree-shown"></div>
    </div>
    <div id="rightMembers">
        <div id="member-toolbars">
            <div id="member-display">
                <div class="member-access-display"><input type="checkbox" value="member-protected" onchange="toggleDisplay(this)"/>Protected</div>
                <div class="member-access-display"><input type="checkbox" value="member-private" onchange="toggleDisplay(this)"/>Private</div>
            </div>
            <div id="toolbar">
                <div id="toggle-class-tree">&nbsp;</div>
                {{#members}}
                    {{#is $type "configs"}}
                        <div class="toolbarButton">
                            <a href="#configs">configs</a>
                        </div>
                    {{/is}}
                    {{#is $type "properties"}}
                        <div class="toolbarButton">
                            <a href="#properties">properties</a>
                        </div>
                    {{/is}}
                    {{#is $type "static-properties"}}
                        <div class="toolbarButton">
                            <a href="#static-properties">static-properties</a>
                        </div>
                    {{/is}}
                    {{#is $type "methods"}}
                        <div class="toolbarButton">
                            <a href="#methods">methods</a>
                        </div>
                    {{/is}}
                    {{#is $type "static-methods"}}
                        <div class="toolbarButton">
                            <a href="#static-methods">static methods</a>
                        </div>
                    {{/is}}
                    {{#is $type "events"}}
                        <div class="toolbarButton">
                            <a href="#events">events</a>
                        </div>
                    {{/is}}
                    {{#is $type "mixins"}}
                        <div class="toolbarButton">
                            <a href="#mixins">mixins</a>
                        </div>
                    {{/is}}
                    {{#is $type "vars"}}
                        <div class="toolbarButton">
                            <a href="#vars">css variables</a>
                        </div>
                    {{/is}}
                {{/members}}
                <div class="memberFilter">
                    <input placeholder="Filter Members" type="search" id="box" name="box">
                </div>
            </div>
        </div>
        <div id="member-toolbars-placeholder"></div>
        <div class="classText"><h2>Summary</h2>{{{classText}}}</div>
        <a href="#" id="back-to-top" style="display: none;">Back to Top </a>
        <div class="members">
            {{#members}}
                <h2 id="{{{$type}}}" class="type">{{{$type}}}</h2>
                <a name="{{{$type}}}"></a>
                {{#items}}
                    <div id="{{{$type}}}-{{{name}}}" class="classmembers member{{#access}}-{{{.}}}{{/access}}" data-member-name="{{{name}}}" {{#accessor}} data-accessor="true"{{/accessor}}{{#inheritdoc}} data-inherited="true"{{/inheritdoc}}>
                        <h2>
                            <a href="#{{{$type}}}-{{{name}}}" class="member-name">
                                {{{name}}}

                                {{#if hasParams}}<span class="params-list">({{/if}}
                                {{#items}}
                                    {{#is $type "param"}}
                                        {{#unless @first}}, {{/unless}}
                                        {{{name}}}
                                        {{#if @last}}
                                            {{#is ../$type "event"}}, eOpts{{/is}}
                                        {{/if}}
                                    {{/is}}
                                {{/items}}
                                {{#if hasParams}}
                                    )</span>
                                {{/if}}
                            </a>
                            {{#type}}
                                <span class='memberType'>{{{.}}}</span>
                            {{/type}}
                            {{#chainable}}
                                <span class='chainable'>chainable</span>
                            {{/chainable}}
                            {{#preventable}}
                                <span class='preventable'>preventable</span>
                            {{/preventable}}
                            {{#readonly}}
                                <span class='readonly'>readonly</span>
                            {{/readonly}}
                            {{#static}}
                                <span class='static'>static</span>
                            {{/static}}
                            {{#required}}
                                <span class='required'>required</span>
                            {{/required}}
                            {{#template}}
                                <span class='template'>template</span>
                            {{/template}}
                            {{#access}}
                                <span class='{{{.}}}'>{{{.}}}</span>
                            {{/access}}
                        </h2>
                        {{#text}}
                            <p>{{{.}}}</p>
                        {{/text}}
                        {{#value}}
                            <p class="defaults-to-dec">Defaults to: {{{.}}}</p>
                        {{/value}}
                        {{#items}}
                            {{#is $type "param"}}
                                {{#if @first}}
                                    <h3>Parameters</h3>
                                {{/if}}

                                <p>{{{name}}} : {{{type}}}</p>
                                <p>{{{text}}}</p>
                            {{/is}}

                            {{#is $type "property"}}
                                {{#unless @index}}
                                    <h3>Properties</h3>
                                {{/unless}}

                                <p>{{{name}}} : {{{type}}}</p>
                                <p>{{{text}}}</p>
                            {{/is}}

                            {{#is $type "return"}}
                                <h3>Returns</h3>
                                <p>{{{type}}}</p>
                                <p>{{{text}}}</p>
                            {{/is}}

                            {{#if @last}}
                                {{#is ../$type "event"}}
                                    <p>eOpts : <a href="Object.html">Object</a></p>
                                    <p>The options object passed to <a href="Ext.util.Observable.html">Ext.util.Observable.addListener</a>.</p>
                                {{/is}}
                            {{/if}}
                        {{/items}}
                        {{#deprecatedMessage}}
                            <p class="deprecated-dec">Deprecated: {{{.}}}</p>
                        {{/deprecatedMessage}}
                        {{#preventable}}
                            <p class='preventable-dec'>This action following this event is preventable. When any
                                of the listeners returns false, the action is cancelled.</p>
                        {{/preventable}}
                        {{#removedVersion}}
                            <p class='removed-dec'>This method has been REMOVED since {{{.}}}</p>
                        {{/removedVersion}}
                        {{#template}}
                            <p class='template-dec'>This is a template method. a hook into the functionality of this
                                class. Feel free to override it in child classes.</p>
                        {{/template}}
                    </div>
                {{/items}}
            {{/members}}
        </div>
    </div>
</div>
</body>
<script type="text/javascript">
    var ExtL = ExtL || {};

    ExtL.treeData = {{{tree}}};
</script>
<script type="text/javascript" src="js/app.js"></script>
</html>
