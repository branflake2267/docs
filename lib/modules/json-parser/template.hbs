<html>
<head>
    <title>{{{name}}}</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="css/app.css">
</head>
<body>
    <h1 class='{{classType}}'>{{{name}}}
        {{#classAlias}}
        <span class='alias'>alias: {{{.}}}</span>
        {{/classAlias}}
    </h1>
    <div class="classHead">
        <div class="classMetaHeader">
            Related Classes <a class="button" href="javascript:void(0);" onClick="ExtL.onExpandToggleClick(this);">collapse</a>
        </div>
        <div class="classMeta">
            {{#if altNames}}
                <div class="classMetaColumn">
                    <h3>Alternate Names</h3>
                    <p class="list">{{{altNames}}}</p>
                </div>
            {{/if}}
            {{#if mixins}}
                <div class="classMetaColumn">
                    <h3>Inherited Mixins</h3>
                    <p class="list">{{{mixins}}}</p>
                </div>
            {{/if}}
            {{#if requires}}
                <div class="classMetaColumn">
                    <h3>Requires</h3>
                    <p class="list">{{{requires}}}</p>
                </div>
            {{/if}}
        </div>
    </div>
    <div id="centerContent">
        <div id="tree-collapse"><a href="#">Toggle Tree</a></div>
        <div id="member-display">
            <div class="member-access-display"><input type="checkbox" value="member-protected" onchange="toggleDisplay(this)"/>Protected</div>
            <div class="member-access-display"><input type="checkbox" value="member-private" onchange="toggleDisplay(this)"/>Private</div>
        </div>
        <div id="tree" class="tree-shown"></div>
        <div id="rightMembers">
            <div id="toolbar">
                {{#members}}
                    {{#is $type "configs"}}
                        <div class="toolbarButton">
                            <a href="#configs">configs</a>
                        </div>
                    {{/is}}
                    {{#is $type "properties"}}
                        <div class="toolbarButton">
                            <a href="#properties">properties</a>
                        </div>
                    {{/is}}
                    {{#is $type "static-properties"}}
                            <div class="toolbarButton">
                                <a href="#static-properties">static-properties</a>
                            </div>
                    {{/is}}
                    {{#is $type "methods"}}
                        <div class="toolbarButton">
                            <a href="#methods">methods</a>
                        </div>
                    {{/is}}
                    {{#is $type "static-methods"}}
                        <div class="toolbarButton">
                            <a href="#static-methods">static methods</a>
                        </div>
                    {{/is}}
                    {{#is $type "events"}}
                        <div class="toolbarButton">
                            <a href="#events">events</a>
                        </div>
                    {{/is}}
                    {{#is $type "mixins"}}
                        <div class="toolbarButton">
                            <a href="#mixins">mixins</a>
                        </div>
                    {{/is}}
                    {{#is $type "vars"}}
                        <div class="toolbarButton">
                            <a href="#vars">vars</a>
                        </div>
                    {{/is}}
                {{/members}}
                <div class="memberFilter">
                    <input placeholder="Filter Members" type="search" id="box" name="box">
                </div>
            </div>
            <div class="classText"><h2>Summary</h2>{{{classText}}}</div>
            <a href="#" id="back-to-top" style="display: none;">Back to Top </a>
            <div class="members">
                {{#members}}
                    <h2 id="{{{$type}}}" class="type">{{{$type}}}</h2>
                    <a name="{{{$type}}}"></a>
                    {{#items}}
                        <div class="classmembers member{{#access}}-{{{.}}}{{/access}}" data-member-name="{{{name}}}" {{#accessor}} data-accessor="true"{{/accessor}}{{#inheritdoc}} data-inherited="true"{{/inheritdoc}}>
                            <a name="{{{name}}}"></a>
                            <h2>
                                <a href="#{{{$type}}}-{{{name}}}" class="member-name">{{{name}}}</a>
                                {{#type}}
                                    <span class='memberType'>{{{.}}}</span>
                                {{/type}}
                                {{#access}}
                                    <span class='{{{.}}}'>{{{.}}}</span>
                                {{/access}}
                            </h2>
                            {{#text}}
                                <p>{{{.}}}</p>
                            {{/text}}
                            {{#value}}
                                <p>Defaults to: {{{.}}}</p>
                            {{/value}}
                            {{#deprecatedMessage}}
                                <p>Deprecated: {{{.}}}</p>
                            {{/deprecatedMessage}}
                        </div>
                    {{/items}}
                {{/members}}
            </div>
        </div>
    </div>
</body>
<script type="text/javascript">
var ExtL = ExtL || {};

ExtL.treeData = {{{tree}}};
</script>
<script type="text/javascript" src="js/app.js"></script>
</html>
