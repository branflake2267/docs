{
  "version": 1,
  "profiles": {
    "js": {
      "language": "javascript",
      "builtins": [
        "Mixed",
        "Event",
        "HTMLElement",
        "undefined",
        "null",
        "Window",
        "Error",
        "CSSStyleRule",
        "CSSStyleSheet",
        "Arguments",
        "Integer",
        "XMLElement",
        "TextNode",
        "Uint8Array",
        "NodeList",
        "XMLHttpRequest",
        "FileList",
        "measurement",
        "percent",
        "List",
        "north", "east", "west", "south", "center",
        "top", "left", "right", "bottom",
        "HTMLCanvasElement",
        "Region"
      ]
    },
    "sass": {
      "undocumented": true,
      "language": "sass",
      "builtins": [
        "Color",
        "color",
        "list"
      ]
    }
  },
  "sources": [{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/core/src",
      "{apiSourceDir}/ext/packages/core/overrides"
    ],
    "defaults" : {
      "class": {
        "package": "core"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{_myRoot}/../docs/js/primitives",

      "{apiSourceDir}/ext/classic/classic/src",
      "{apiSourceDir}/ext/classic/classic/overrides",

      "{apiSourceDir}/ext/.sencha/package/Boot.js",
      "{apiSourceDir}/ext/.sencha/package/Microloader.js"
    ]
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/charts/src",
      "{apiSourceDir}/ext/packages/charts/classic/src",
      "{apiSourceDir}/ext/packages/charts/classic/overrides"
    ],
    "defaults" : {
      "class": {
        "package": "charts"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/amf/src"
    ],
    "defaults" : {
      "class": {
        "package": "amf"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/soap/src"
    ],
    "defaults" : {
      "class": {
        "package": "soap"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/packages/pivot/src",
      "{apiSourceDir}/packages/pivot/classic/src"
    ],
    "defaults" : {
      "class": {
        "package": "pivot",
        "tier": "premium"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/packages/exporter/src",
      "{apiSourceDir}/packages/exporter/classic/src"
    ],
    "defaults" : {
      "class": {
        "package": "exporter",
        "tier": "premium"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/ux/src",
      "{apiSourceDir}/ext/packages/ux/classic/src"
    ],
    "defaults" : {
      "class": {
        "package": "ux"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/classic/theme-aria/sass/src",
      "{apiSourceDir}/ext/classic/theme-aria/sass/var",
      "{apiSourceDir}/ext/classic/theme-aria/sass/etc",
      "{apiSourceDir}/ext/classic/theme-aria/sass/all",

      "{apiSourceDir}/ext/classic/theme-base/sass/src",
      "{apiSourceDir}/ext/classic/theme-base/sass/var",
      "{apiSourceDir}/ext/classic/theme-base/sass/etc",

      "{apiSourceDir}/ext/classic/theme-classic/sass/src",
      "{apiSourceDir}/ext/classic/theme-classic/sass/var",
      "{apiSourceDir}/ext/classic/theme-classic/sass/etc",

      "{apiSourceDir}/ext/classic/theme-classic-sandbox/sass/src",
      "{apiSourceDir}/ext/classic/theme-classic-sandbox/sass/var",
      "{apiSourceDir}/ext/classic/theme-classic-sandbox/sass/etc",

      "{apiSourceDir}/ext/classic/theme-crisp/sass/src",
      "{apiSourceDir}/ext/classic/theme-crisp/sass/var",
      "{apiSourceDir}/ext/classic/theme-crisp/sass/etc",

      "{apiSourceDir}/ext/classic/theme-crisp-touch/sass/src",
      "{apiSourceDir}/ext/classic/theme-crisp-touch/sass/var",
      "{apiSourceDir}/ext/classic/theme-crisp-touch/sass/etc",

      "{apiSourceDir}/ext/classic/theme-gray/sass/src",
      "{apiSourceDir}/ext/classic/theme-gray/sass/var",
      "{apiSourceDir}/ext/classic/theme-gray/sass/etc",

      "{apiSourceDir}/ext/classic/theme-neptune/sass/src",
      "{apiSourceDir}/ext/classic/theme-neptune/sass/var",
      "{apiSourceDir}/ext/classic/theme-neptune/sass/etc",

      "{apiSourceDir}/ext/classic/theme-neptune-touch/sass/src",
      "{apiSourceDir}/ext/classic/theme-neptune-touch/sass/var",
      "{apiSourceDir}/ext/classic/theme-neptune-touch/sass/etc",

      "{apiSourceDir}/ext/classic/theme-neutral/sass/src",
      "{apiSourceDir}/ext/classic/theme-neutral/sass/var",
      "{apiSourceDir}/ext/classic/theme-neutral/sass/etc",

      "{apiSourceDir}/ext/classic/theme-triton/sass/src",
      "{apiSourceDir}/ext/classic/theme-triton/sass/var",
      "{apiSourceDir}/ext/classic/theme-triton/sass/etc",

      "{apiSourceDir}/ext/classic/touch-sizing/sass/etc",
      "{apiSourceDir}/ext/classic/touch-sizing/sass/var",

      "{apiSourceDir}/ext/packages/core/sass/etc",
      "{apiSourceDir}/ext/packages/core/sass/src",
      "{apiSourceDir}/ext/packages/core/sass/var",

      "{apiSourceDir}/ext/packages/charts/sass/etc",
      "{apiSourceDir}/ext/packages/charts/sass/src",
      "{apiSourceDir}/ext/packages/charts/sass/var",

      "{apiSourceDir}/ext/packages/font-awesome/sass/etc",
      "{apiSourceDir}/ext/packages/font-awesome/sass/src",

      "{apiSourceDir}/ext/packages/font-ext/sass/src",

      "{apiSourceDir}/ext/packages/font-pictos/sass/src"
    ]
  }],
  "outputs": {
    "all": {
      "default": true,
      "depends": [ "combo-nosrc", "all-classes" ]
    },
    "combo-nosrc": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/{toolkit}-json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/all-classes",
      "file": "classic_all-classes.json",
      "includeSrc": true,
      "links": true,
      "flatten": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes-flatten": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/all-classes-flatten",
      "file": "classic_all-classes-flatten.json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "pretty": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    }
  }
}
