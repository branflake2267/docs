{
  "version": 1,
  "profiles": {
    "js": {
      "undocumented": true,
      "language": "javascript",
      "builtins": [
        "Mixed",
        "Event",
        "HTMLElement",
        "undefined",
        "null",
        "Window",
        "Error",
        "CSSStyleRule",
        "CSSStyleSheet",
        "Arguments",
        "Integer",
        "XMLElement",
        "TextNode",
        "Uint8Array",
        "NodeList",
        "XMLHttpRequest",
        "FileList",
        "measurement",
        "percent",
        "List",
        "HTMLCanvasElement",
        "Region"
      ]
    },
    "sass": {
      "undocumented": true,
      "language": "sass",
      "builtins": [
        "Color",
        "color",
        "list"
      ]
    }
  },
  "sources": [{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/core/src",
      "{apiSourceDir}/ext/packages/core/overrides"
    ],
    "defaults" : {
      "class": {
        "package": "core"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{_myRoot}/../docs/js/primitives",
      "{_myRoot}/../docs/js/enums",

      "{apiSourceDir}/ext/modern/modern/src",
      "{apiSourceDir}/modern/modern/overrides",

      "{apiSourceDir}/.sencha/package/Boot.js",
      "{apiSourceDir}/.sencha/package/Microloader.js"
    ]
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/charts/src",
      "{apiSourceDir}/ext/packages/charts/modern/src",
      "{apiSourceDir}/ext/packages/charts/modern/overrides"
    ],
    "defaults" : {
      "class": {
        "package": "charts"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/amf/src"
    ],
    "defaults" : {
      "class": {
        "package": "amf"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/soap/src"
    ],
    "defaults" : {
      "class": {
        "package": "soap"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/ux/src"
    ],
    "defaults" : {
      "class": {
        "package": "ux"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-base/sass/src",
      "{apiSourceDir}/ext/modern/theme-base/sass/var",
      "{apiSourceDir}/ext/modern/theme-base/sass/etc",

      "{apiSourceDir}/ext/modern/theme-cupertino/sass/src",
      "{apiSourceDir}/ext/modern/theme-cupertino/sass/var",
      "{apiSourceDir}/ext/modern/theme-cupertino/sass/etc",

      "{apiSourceDir}/ext/modern/theme-mountainview/sass/src",
      "{apiSourceDir}/ext/modern/theme-mountainview/sass/var",
      "{apiSourceDir}/ext/modern/theme-mountainview/sass/etc",

      "{apiSourceDir}/ext/modern/theme-neptune/sass/src",
      "{apiSourceDir}/ext/modern/theme-neptune/sass/var",
      "{apiSourceDir}/ext/modern/theme-neptune/sass/etc",

      "{apiSourceDir}/ext/modern/theme-windows/sass/src",
      "{apiSourceDir}/ext/modern/theme-windows/sass/var",
      "{apiSourceDir}/ext/modern/theme-windows/sass/etc",

      "{apiSourceDir}/ext/packages/core/sass/etc",
      "{apiSourceDir}/ext/packages/core/sass/src",
      "{apiSourceDir}/ext/packages/core/sass/var",

      "{apiSourceDir}/ext/packages/charts/sass/etc",
      "{apiSourceDir}/ext/packages/charts/sass/src",
      "{apiSourceDir}/ext/packages/charts/sass/var",

      "{apiSourceDir}/ext/packages/font-awesome/sass/etc",
      "{apiSourceDir}/ext/packages/font-awesome/sass/src",

      "{apiSourceDir}/ext/packages/font-pictos/sass/src"
    ]
  }],
  "outputs": {
    "all": {
      "default": true,
      "depends": [ "combo-nosrc", "all-classes" ]
    },
    "combo-nosrc": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/{toolkit}-json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/{toolkit}-json",
      "file": "_all-classes.json",
      "includeSrc": true,
      "links": true,
      "flatten": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes-flatten": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/{toolkit}-json",
      "file": "_all-classes-flatten.json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "pretty": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    }
  }
}
