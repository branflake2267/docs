{
  "version": 1,
  "profiles": {
    "js": {
      "language": "javascript",
      "builtins": [
        "Mixed",
        "Event",
        "HTMLElement",
        "undefined",
        "null",
        "Window",
        "Error",
        "CSSStyleRule",
        "CSSStyleSheet",
        "Arguments",
        "Integer",
        "XMLElement",
        "TextNode",
        "Uint8Array",
        "NodeList",
        "XMLHttpRequest",
        "FileList",
        "HTMLCanvasElement",
        "List",
        "Region",
        "d3.selection",
        "measurement",
        "percent",
        "top", "left", "right", "bottom"
      ]
    },
    "sass": {
      "language": "sass",
      "builtins": [
        "Color",
        "color",
        "list"
      ]
    }
  },
  "sources": [{
    "profile": "js",
    "path": [
      "{apiSourceDir}/extreact/"
    ],
    "defaults" : {
      "class": {
        "package": "extreact"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{_myRoot}/../docs/js/primitives"
    ]
  }],
  "outputs": {
    "all": {
      "default": true,
      "depends": [ "combo-nosrc", "all-classes" ]
    },
    "combo-nosrc": {
      "type": "json",
      "dir": "{apiInputDir}/extreact/1.0.0/extreact-json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes": {
      "type": "json",
      "dir": "{apiInputDir}/extreact/1.0.0/extreact-json",
      "file": "_all-classes.json",
      "includeSrc": true,
      "links": true,
      "flatten": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes-flatten": {
      "type": "json",
      "dir": "{apiInputDir}/extreact/1.0.0/extreact-json",
      "file": "_all-classes-flatten.json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "pretty": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    }
  }
}
