{
  "version": 1,
  "profiles": {
    "js": {
      "language": "javascript",
      "builtins": [
        "Mixed",
        "Event",
        "HTMLElement",
        "undefined",
        "null",
        "Window",
        "Error",
        "CSSStyleRule",
        "CSSStyleSheet",
        "Arguments",
        "Integer",
        "XMLElement",
        "TextNode",
        "Uint8Array",
        "NodeList",
        "XMLHttpRequest",
        "FileList",
        "HTMLCanvasElement",
        "List",
        "Region",
        "d3.selection",
        "measurement",
        "percent",
        "top", "left", "right", "bottom"
      ]
    },
    "sass": {
      "language": "sass",
      "builtins": [
        "Color",
        "color",
        "list"
      ]
    }
  },
  "sources": [{
    "profile": "js",
    "path": [
      "{apiSourceDir}/reactor/"
    ],
    "defaults" : {
      "class": {
        "package": "reactor"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{_myRoot}/../docs/js/primitives"
    ]
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/packages/core/sass/etc",
      "{apiSourceDir}/ext/packages/core/sass/src",
      "{apiSourceDir}/ext/packages/core/sass/var",

      "{apiSourceDir}/ext/packages/charts/sass/etc",
      "{apiSourceDir}/ext/packages/charts/sass/src",
      "{apiSourceDir}/ext/packages/charts/sass/var",

      "{apiSourceDir}/ext/packages/font-awesome/sass/etc",
      "{apiSourceDir}/ext/packages/font-awesome/sass/src",

      "{apiSourceDir}/ext/packages/font-pictos/sass/src",

      "{apiSourceDir}/packages/calendar/sass/etc",
      "{apiSourceDir}/packages/calendar/sass/src",
      "{apiSourceDir}/packages/calendar/sass/var",

      "{apiSourceDir}/packages/d3/sass/etc",
      "{apiSourceDir}/packages/d3/sass/src",
      "{apiSourceDir}/packages/d3/sass/var",

      "{apiSourceDir}/packages/exporter/sass/etc",

      "{apiSourceDir}/packages/pivot/sass/etc",
      "{apiSourceDir}/packages/pivot/sass/src",
      "{apiSourceDir}/packages/pivot/sass/var",

      "{apiSourceDir}/ext/packages/google/modern/sass/src"
    ]
  }],
  "outputs": {
    "all": {
      "default": true,
      "depends": [ "combo-nosrc", "all-classes" ]
    },
    "combo-nosrc": {
      "type": "json",
      "dir": "{apiInputDir}/extreact/1.0.0/extreact-json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes": {
      "type": "json",
      "dir": "{apiInputDir}/extreact/1.0.0/extreact-json",
      "file": "_all-classes.json",
      "includeSrc": true,
      "links": true,
      "flatten": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes-flatten": {
      "type": "json",
      "dir": "{apiInputDir}/extreact/1.0.0/extreact-json",
      "file": "_all-classes-flatten.json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "pretty": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    }
  }
}
