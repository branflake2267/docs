
{
  "version": 1,
  "profiles": {
    "js": {
      "language": "javascript",
      "builtins": [
        "Mixed",
        "Event",
        "HTMLElement",
        "undefined",
        "null",
        "Window",
        "Error",
        "CSSStyleRule",
        "CSSStyleSheet",
        "Arguments",
        "Integer",
        "XMLElement",
        "TextNode",
        "Uint8Array",
        "NodeList",
        "XMLHttpRequest",
        "FileList",
        "measurement",
        "Ext.Element",
        "Ext.view.View",
        "Ext.Component",
        "Ext.ux.event.Driver"
      ]
    }
  },
  "sources": [{
    "profile": "js",
    "path": [
      "{_myRoot}/../docs/js/primitives",
      "{apiSourceDir}/node_modules/orion-core/serve"
    ]
  }],
  "outputs": {
    "all": {
      "default": true,
      "depends": [ "combo-nosrc", "all-classes" ]
    },
    "all-classes": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/json",
      "file": "_all-classes.json",
      "includeSrc": true,
      "links": true,
      "flatten": false,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "combo-nosrc": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/json",
      "includeSrc": true,
      "links": true,
      "flatten": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    }
  }
}
