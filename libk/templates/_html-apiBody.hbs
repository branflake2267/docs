<div id="member-toolbars">
    <div id="member-class-name">
        {{#cls.clsSpec}}
            <span class='{{.}}-icon'></span>
        {{/cls.clsSpec}}

        <h1 class='{{cls.$type}} black-50 truncated fw4 f3'>
            {{#if multSrc}}
                <span class="multi-src-btn">{{{name}}}</span>
            {{else}}
                <a class="link blue underline-hover" href="{{{fileName}}}" target="_blank">{{{name}}}</a>
            {{/if}}
            {{#cls.alias}}
                <span class='alias f6'>{{../cls.aliasPrefix}}: {{../cls.aliasName}}</span>
            {{/cls.alias}}
            {{#cls.access}}
                <span class="member-tags"><span class="{{.}} full-flag">{{.}}</span></span>
            {{/cls.access}}
            {{#cls.isDeprecated}}
                <span class="member-tags"><span class="deprecated full-flag">deprecated</span></span>
            {{/cls.isDeprecated}}
            {{#cls.isRemoved}}
                <span class="member-tags"><span class="removed full-flag">removed</span></span>
            {{/cls.isRemoved}}
            {{#cls.singleton}}
                <span class="member-tags"><span class='singleton'>singleton</span></span>
            {{/cls.singleton}}
            {{#cls.abstract}}
                <span class="member-tags"><span class='abstract'>abstract</span></span>
            {{/cls.abstract}}
            {{#cls.isEnum}}
                <span class='alias'>enum of <b>String</b></span>
            {{/cls.isEnum}}
            {{#cls.tier}}
                <span class="member-tags"><span class="{{{tier}}}">{{{tier}}}</span></span>
            {{/cls.tier}}
        </h1>
    </div>
</div>
<div class="context-menu-ct">
    <div class="context-header navy ma0 dn">
        <div id="hide-context-menu" class="fa fa-caret-right white f4 pointer tc "></div>
    </div>
    <div class="search absolute ma0 f">
        <form autocomplete="off">
            {{#prodVerMeta.hasToolkits}}
                <div class="toolkit-search-filter-ct">
                    <span id="modern-search-filter" class="tooltip tooltip-tr-br active" data-tip="Include Modern Search Results"><div class="callout callout-b"></div></span>
                    <span id="classic-search-filter" class="tooltip tooltip-tr-br active" data-tip="Include Classic Search Results"><div class="callout callout-b"></div></span>
                </div>
            {{/prodVerMeta.hasToolkits}}

            <input class="br2 ba b--light-gray" type="text" type="search" placeholder="search" id="searchtext">
            <i id="filtersubmit" class="fa fa-search fa fa-search absolute z-1 gray right-2 mt1"></i>
            <span id="help-btn" data-tip="Docs Help" class="tooltip tooltip-tr-br"><div class="callout callout-b tooltip tooltip-tr-br relative pointer fa fa-question-circle white f3 v-mid pl1"></div></span>
        </form>
    </div>
    <div class="context-menu-tabs tracked b z-1 dn absolute left-0 right-0">
        <div id="filterTab" class="toolbarHeaderButton active-tab">
            Filters
        </div>
        <div id="relatedClassesTab" class="toolbarHeaderButton dib bg-white color dark-gray br2 f6 pointer">
            Related Classes
        </div>
    </div>
    <div id="filters-ct">
        <div id="toolbar" class="bg-near-white bt bb b--light-gray relative cf">
            <div class="memberFilter fr">
                <input placeholder="Filter members..." type="search" id="member-filter-field" name="member-filter-field">
                <div id="toggleExamples" class="fa fa-code tooltip tooltip-tr-br bg-navy white fr pointer br2 relative z-0" data-expand="Expand All Examples" data-collapse="Collapse All Examples">
                    <div class="callout callout-b"></div>
                </div>
                <div id="toggleAll" class="fa fa-plus tooltip tooltip-tr-br bg-navy white fr pointer br2 relative z-0" data-expand="Expand All Members" data-collapse="Collapse All Members">
                    <div class="callout callout-b"></div>
                </div>
            </div>
            <div id="member-display" class="bg-white absolute right-0 ">
                <div class="member-access-display dib mr3"><label class="pointer">
                    <input id="publicCheckbox" type="checkbox" value="member-public" checked=true/>Public</label>
                </div>
                <div class="member-access-display dib mr3">
                    <label class="pointer"><input id="protectedCheckbox" type="checkbox" value="member-protected" checked=true/>Protected</label>
                </div>
                <div class="member-access-display dib mr3">
                    <label class="pointer"><input id="privateCheckbox" type="checkbox" value="member-private" checked=true/>Private</label>
                </div>
                <div class="member-access-display dib mr3"><label class="pointer">
                    <input id="inheritedCheckbox" type="checkbox" value="member-inherited" checked=true/>Inherited</label>
                </div>
            </div>
            <div id="member-types-menu" class="member-types-menu dib">
            {{#cls.memberTypeGroups}}
                {{#is $type "configs"}}
                    <a href="#configs" id="configs-button-link" class="black link"><div id="configs-nav-btn" class="toolbarButton disabled dib mr3 bg-near-white relative br2">
                        <i class="fa fa-cubes"></i>
                        <span class="member-type-btn-text black">configs</span>
                        <div id="configs-count" class="nav-btn-count absolute navy bg-near-white f6 b ba b--navy"></div>
                        <div class="nav-btn-mask absolute top-0 left-0"></div>
                    </div></a>
                {{/is}}
                {{#is $type "properties"}}
                    <a href="#properties" id="properties-button-link"><div id="properties-nav-btn" class="toolbarButton disabled">
                        <i class="fa fa-book"></i>
                        <span class="member-type-btn-text">properties</span>
                        <div id="properties-count" class="nav-btn-count"></div>
                        <div class="nav-btn-mask"></div>
                    </div></a>
                {{/is}}
                {{#is $type "methods"}}
                    <a href="#methods" id="methods-button-link"><div id="methods-nav-btn" class="toolbarButton disabled">
                        <i class="fa fa-object-group"></i>
                        <span class="member-type-btn-text">methods</span>
                        <div id="methods-count" class="nav-btn-count"></div>
                        <div class="nav-btn-mask"></div>
                    </div></a>
                {{/is}}
                {{#is $type "events"}}
                    <a href="#events" id="events-button-link"><div id="events-nav-btn" class="toolbarButton disabled">
                        <i class="fa fa-bolt"></i>
                        <span class="member-type-btn-text">events</span>
                        <div id="events-count" class="nav-btn-count"></div>
                        <div class="nav-btn-mask"></div>
                    </div></a>
                {{/is}}
                {{#is $type "vars"}}
                    <a href="#vars" id="vars-button-link"><div id="vars-nav-btn" class="toolbarButton disabled">
                        <i class="fa fa-paint-brush"></i>
                        <span class="member-type-btn-text">theme variables</span>
                        <div id="vars-count" class="nav-btn-count"></div>
                        <div class="nav-btn-mask"></div>
                    </div></a>
                {{/is}}
                {{#is $type "sass-mixins"}}
                    <a href="#sass-mixins" id="sass-mixins-button-link"><div id="sass-mixins-nav-btn" class="toolbarButton disabled">
                        <i class="fa fa-tint"></i>
                        <span class="member-type-btn-text">theme mixins</span>
                        <div id="sass-mixins-count" class="nav-btn-count"></div>
                        <div class="nav-btn-mask"></div>
                    </div></a>
                {{/is}}
            {{/cls.memberTypeGroups}}
            </div>
        </div>
    </div>
    <div id="related-classes-context-ct" class="dn">
        <!-- We'll programmatically copy in the related classes elements here on page load -->
    </div>
</div>
<div id="class-body-wrap" class="class-body-wrap absolute bottom-0 left-0 right-0 overflow-x-hidden overflow-y-auto bg-white z-0">
    <div class="body-wrap-inner min-h-100">
        <div id="classText" class="classText lh-copy cf">
            <div class="classHead">
                <div class="classMeta ba bw1 b--light-gray black-60 fr bg-light-gray pa3 overflow-hidden">
                    {{#if altNames}}
                        <div class="classMetaColumn">
                            <h3>Alternate Names</h3>
                            <p class="list">{{{altNames}}}</p>
                        </div>
                    {{/if}}
                    {{#if extends}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Hierarchy</h3>
                            {{{extends}}}
                        </div>
                    {{/if}}
                    {{#if localMixins}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Mixins</h3>
                            <p class="list black-60 lh-copy">{{{localMixins}}}</p>
                        </div>
                    {{/if}}
                    {{#if mixins}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Inherited Mixins</h3>
                            <p class="list black-60 lh-copy">{{{mixins}}}</p>
                        </div>
                    {{/if}}
                    {{#if requires}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Requires</h3>
                            <p class="list black-60 lh-copy">{{{requires}}}</p>
                        </div>
                    {{/if}}
                    {{#if uses}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Uses</h3>
                            <p class="list black-60 lh-copy">{{{uses}}}</p>
                        </div>
                    {{/if}}
                    {{#if extenders}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Sub-Classes</h3>
                            <p class="list black-60 lh-copy">{{{extenders}}}</p>
                        </div>
                    {{/if}}
                    {{#if mixers}}
                        <div class="classMetaColumn">
                            <h3 class="ma0">Mixed Into</h3>
                            <p class="list black-60 lh-copy">{{{mixers}}}</p>
                        </div>
                    {{/if}}
                </div>
            </div>
            {{#isDeprecated}}
                {{#deprecatedMessage}}
                    <div class="deprecated-dec">This class has been <span class="member-tags"><span class="deprecated">deprecated</span>{{#deprecatedVersion}} since {{.}}{{/deprecatedVersion}}</span>
                        {{{.}}}</div>
                {{/deprecatedMessage}}
            {{/isDeprecated}}
            {{#is classAccess "private"}}
                <div class="privatecls-dec">
                    NOTE: This is a private utility class for internal use by the framework. Don't rely on its existence.
                </div>
            {{/is}}
            {{#if classText}}
                <h2>Summary</h2>
                {{{classText}}}
            {{/if}}
            {{#if isEnum}}
                <div class="enum-msg">ENUM: This enumeration defines a set of String values. It exists primarily for documentation purposes - in code use the actual string values like '{{enumProperty}}', don't reference them through this class {{name}}.{{enumProperty}}.</div>
            {{/if}}
        </div>
        <div class="no-visible-members-warning tc white bg-light-red b cf pv4 dn">No members found using the current filters</div>
        <div class="members cf bt b--light-gray">
            {{#if hasConfigs}}
                <div id="configs-ct">
                    <h2 id="configs" class="type blue mt1 lh-copy tracked ttu f4 bb bw3 b--black-30 fw4">configs</h2>
                    <a class="link underline-hover blue" name="configs"></a>
                    {{#if configs.hasRequiredConfigs}}
                        <div id="required-configs-ct">
                            <div class="type-sub-category-title black-60 bg-light-gray tracked ttu f6 pv2 ph3">Required Configs</div>
                            {{#configs.requiredConfigs}}
                                {{> _api-member}}
                            {{/configs.requiredConfigs}}
                        </div>
                    {{/if}}

                    {{#if configs.hasOptionalConfigs}}
                        <div id="optional-configs-ct">
                            <div class="type-sub-category-title black-60 bg-light-gray tracked ttu f6 pv2 ph3">Optional Configs</div>
                            {{#configs.optionalConfigs}}
                                {{> _api-member}}
                            {{/configs.optionalConfigs}}
                        </div>
                    {{/if}}
                </div>
            {{/if}}

            {{#if hasProperties}}
                <div id="properties-ct">
                    <h2 id="properties" class="type blue mt1 lh-copy tracked ttu f4 bb bw3 b--black-30 fw4">properties</h2>
                    <a class="link underline-hover blue" name="properties"></a>
                    {{#if properties.hasInstanceProperties}}
                        <div id="instance-properties-ct">
                            <div class="type-sub-category-title black-60 bg-light-gray tracked ttu f6 pv2 ph3">Instance Properties</div>
                            {{#properties.instanceProperties}}
                                {{> _api-member}}
                            {{/properties.instanceProperties}}
                        </div>
                    {{/if}}

                    {{#if properties.hasStaticProperties}}
                        <div id="static-properties-ct">
                            <div class="type-sub-category-title black-60 bg-light-gray tracked ttu f6 pv2 ph3">Static Properties</div>
                            {{#properties.staticProperties}}
                                {{> _api-member}}
                            {{/properties.staticProperties}}
                        </div>
                    {{/if}}
                </div>
            {{/if}}

            {{#if hasMethods}}
                <div id="methods-ct">
                    <h2 id="methods" class="type blue mt1 lh-copy tracked ttu f4 bb bw3 b--black-30 fw4">methods</h2>
                    <a class="link underline-hover blue" name="methods"></a>
                    {{#if methods.hasInstanceMethods}}
                        <div id="instance-methods-ct">
                            <div class="type-sub-category-title black-60 bg-light-gray tracked ttu f6 pv2 ph3">Instance Methods</div>
                            {{#methods.instanceMethods}}
                                {{> _api-member}}
                            {{/methods.instanceMethods}}
                        </div>
                    {{/if}}

                    {{#if methods.hasStaticMethods}}
                        <div id="static-methods-ct">
                            <div class="type-sub-category-title black-60 bg-light-gray tracked ttu f6 pv2 ph3">Static Methods</div>
                            {{#methods.staticMethods}}
                                {{> _api-member}}
                            {{/methods.staticMethods}}
                        </div>
                    {{/if}}
                </div>
            {{/if}}

            {{#if hasEvents}}
                <div id="events-ct">
                    <h2 id="events" class="type blue mt1 lh-copy tracked ttu f4 bb bw3 b--black-30 fw4">events</h2>
                    <a class="link underline-hover blue" name="events"></a>
                    {{#events}}
                        {{> _api-member}}
                    {{/events}}
                </div>
            {{/if}}

            {{#if hasVars}}
                <div id="vars-ct">
                    <h2 id="vars" class="type blue mt1 lh-copy tracked ttu f4 bb bw3 b--black-30 fw4">theme variables</h2>
                    <a class="link underline-hover blue" name="vars"></a>
                    {{#vars}}
                        {{> _api-member}}
                    {{/vars}}
                </div>
            {{/if}}

            {{#if hasSass-mixins}}
                <div id="sass-mixins-ct">
                    <h2 id="sass-mixins" class="type blue mt1 lh-copy tracked ttu f4 bb bw3 b--black-30 fw4">theme mixins</h2>
                    <a class="link underline-hover blue" name="sass-mixins"></a>
                    {{#sass-mixins}}
                        {{> _api-member}}
                    {{/sass-mixins}}
                </div>
            {{/if}}

        </div>
    </div>
    {{> _footer}}
</div>