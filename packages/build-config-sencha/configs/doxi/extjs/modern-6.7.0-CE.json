{
  "version": 1,
  "profiles": {
    "js": {
      "language": "javascript",
      "builtins": [
        "Mixed",
        "Event",
        "HTMLElement",
        "undefined",
        "null",
        "Window",
        "Error",
        "CSSStyleRule",
        "CSSStyleSheet",
        "Arguments",
        "Integer",
        "XMLElement",
        "TextNode",
        "Uint8Array",
        "NodeList",
        "XMLHttpRequest",
        "FileList",
        "measurement",
        "percent",
        "List",
        "HTMLCanvasElement",
        "Region",
        "google.maps.MouseEvent",
        "google.maps.Marker",
        "google.maps.Map",
        "google.maps.LatLng",
        "Map",
        "shadow",
        "map"
      ]
    },
    "sass": {
      "undocumented": true,
      "language": "sass",
      "builtins": [
        "Color",
        "color",
        "list"
      ]
    }
  },
  "sources": [{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/core/src",
      "{apiSourceDir}/ext/packages/core/overrides"
    ],
    "defaults" : {
      "class": {
        "package": "core"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{_myRoot}/configs/doxi/extjs/add-types/js/primitives",
      "{_myRoot}/configs/doxi/extjs/add-types/js/enums",

      "{apiSourceDir}/../SDK/ext/modern/modern/src",
      "{apiSourceDir}/../SDK//ext/modern/modern/overrides",

      "{apiSourceDir}/.sencha/package/Boot.js",
      "{apiSourceDir}/.sencha/package/Microloader.js"
    ]
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/packages/calendar/src",
      "{apiSourceDir}/packages/calendar/modern/overrides",
      "{apiSourceDir}/packages/calendar/modern/src"
    ],
    "defaults" : {
      "class": {
        "package": "calendar",
        "tier": "premium"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/../package-loader/packages/package-loader/src"
    ],
    "defaults" : {
      "class": {
        "package": "package-loader"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/google/src",
      "{apiSourceDir}/ext/packages/google/modern/src"
    ],
    "defaults" : {
      "class": {
        "package": "google"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/amf/src"
    ],
    "defaults" : {
      "class": {
        "package": "amf"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/soap/src"
    ],
    "defaults" : {
      "class": {
        "package": "soap"
      }
    }
  },{
    "profile": "js",
    "path": [
      "{apiSourceDir}/ext/packages/ux/src"
    ],
    "defaults" : {
      "class": {
        "package": "ux"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-base/sass/src",
      "{apiSourceDir}/ext/modern/theme-base/sass/var",
      "{apiSourceDir}/ext/modern/theme-base/sass/etc"
    ],
    "defaults" : {
      "class": {
        "package": "theme-base"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-material/sass/src",
      "{apiSourceDir}/ext/modern/theme-material/sass/var",
      "{apiSourceDir}/ext/modern/theme-material/sass/etc"
    ],
    "defaults" : {
      "class": {
        "package": "theme-material"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-ios/sass/src",
      "{apiSourceDir}/ext/modern/theme-ios/sass/var",
      "{apiSourceDir}/ext/modern/theme-ios/sass/etc"
    ],
    "defaults" : {
      "class": {
        "package": "theme-ios"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-triton/sass/src",
      "{apiSourceDir}/ext/modern/theme-triton/sass/var",
      "{apiSourceDir}/ext/modern/theme-triton/sass/etc"
    ],
    "defaults" : {
      "class": {
        "package": "theme-triton"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-neptune/sass/src",
      "{apiSourceDir}/ext/modern/theme-neptune/sass/var",
      "{apiSourceDir}/ext/modern/theme-neptune/sass/etc"
    ],
    "defaults" : {
      "class": {
        "package": "theme-neptune"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/modern/theme-windows/sass/src",
      "{apiSourceDir}/ext/modern/theme-windows/sass/var",
      "{apiSourceDir}/ext/modern/theme-windows/sass/etc"
    ],
    "defaults" : {
      "class": {
        "package": "theme-windows"
      }
    }
  },{
    "profile": "sass",
    "path": [
      "{apiSourceDir}/ext/packages/core/sass/etc",
      "{apiSourceDir}/ext/packages/core/sass/src",
      "{apiSourceDir}/ext/packages/core/sass/var",

      "{apiSourceDir}/ext/packages/font-awesome/sass/etc",
      "{apiSourceDir}/ext/packages/font-awesome/sass/src",

      "{apiSourceDir}/ext/packages/font-pictos/sass/src",

      "{apiSourceDir}/packages/calendar/sass/etc",
      "{apiSourceDir}/packages/calendar/sass/src",
      "{apiSourceDir}/packages/calendar/sass/var",

      "{apiSourceDir}/ext/packages/google/modern/sass/src"
    ]
  }],
  "outputs": {
    "all": {
      "default": true,
      "depends": [ "combo-nosrc", "all-classes" ]
    },
    "combo-nosrc": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/{toolkit}-json",
      "includeSrc": true,
      "flatten": true,
      "links": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/all-classes",
      "file": "modern_all-classes.json",
      "includeSrc": true,
      "flatten": false,
      "links": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    },
    "all-classes-flatten": {
      "type": "json",
      "dir": "{apiInputDir}/{product}/{version}/all-classes-flatten",
      "file": "modern_all-classes-flatten.json",
      "includeSrc": true,
      "flatten": true,
      "pretty": false,
      "links": true,
      "autoLinks": [
        "text",
        "deprecatedMessage",
        "removedMessage"
      ],
      "renames": {
        "number"  : "Number",
        "boolean" : "Boolean",
        "string"  : "String"
      }
    }
  }
}
